{
  "compilerOptions": {
    "moduleResolution": "node",
    "removeComments": false,
    "sourceMap": true,
    "target": "ES5",
    "outDir": "build/types",
    "declaration": true,
    "strict": true,
    "module": "ESNext",
    "noEmitOnError": true,
    "lib": ["ES2018"],
    "esModuleInterop": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
