{
  "name": "api",
  "version": "0.2.2",
  "private": "true",
  "description": "",
  "source": "src/server.ts",
  "main": "./dist/server.js",
  "targets": {
    "main": {
      "engines": {
        "node": ">=14.x"
      },
      "includeNodeModules": true,
      "context": "node",
      "optimize": true,
      "isLibrary": false,
      "sourceMap": false
    }
  },
  "scripts": {
    "postinstall": "npm run nexus:generate",
    "build": "npm run clean && npx parcel build",
    "clean": "npx rimraf dist .parcel-cache",
    "watch:nexus": "tsnd --transpile-only src/schema",
    "watch:ts": "tsc --watch",
    "offline": "sls offline",
    "dev": "run-p watch:* offline",
    "nexus:generate": "ts-node --transpile-only src/schema",
    "predeploy": "run-s nexus:generate",
    "deploy": "cross-env NODE_ENV=production && create-env --env-file .env.local --env-prefix PROD_ && npm run build && npx serverless deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/artem-korolev/prismajs-mongodb-graphql-aws-lambda-starter-kit"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.7.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "18.7.18",
    "@types/node-fetch": "2.6.2",
    "create-env": "^1.1.1",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.7.0",
    "prettier": "^2.7.1",
    "serverless": "^3.22.0",
    "serverless-bundle": "^5.5.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-offline": "^10.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "apollo-server-core": "^3.10.2",
    "apollo-server-lambda": "^3.10.2",
    "dotenv": "^16.0.2",
    "graphql": "^16.6.0",
    "nexus": "^1.3.0",
    "prisma-client": "^1.6.2"
  }
}
